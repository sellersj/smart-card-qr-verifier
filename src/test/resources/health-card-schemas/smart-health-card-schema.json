{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://smarthealth.cards/schema/smart-health-card.json",
	"title": "Root",
	"type": "object",
	"required": [
		"verifiableCredential"
	],
	"properties": {
		"verifiableCredential": {
			"$id": "#root/verifiableCredential",
			"title": "Verifiablecredential",
			"type": "array",
			"minItems": 1,
			"items": {
				"$id": "#root/verifiableCredential/items",
				"title": "Items",
				"type": "string",
				"default": "",
				"pattern": "^[a-zA-Z0-9_-]+\\.[a-zA-Z0-9_-]+\\.[a-zA-Z0-9_-]+$"
			}
		}
	}	
}